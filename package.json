{
  "name": "bitelio",
  "version": "1.0.0",
  "description": "web",
  "author": "Guillermo Guirao Aguilar",
  "license": "ISC",
  "scripts": {
    "serve": "parcel src/{index.pug,404.pug,robots.txt,sitemap.txt,favicon.ico}",
    "prebuild": "rm -rf dist",
    "build": "parcel build src/{index.pug,404.pug,robots.txt,sitemap.txt,favicon.ico} --no-source-maps",
    "prepublish": "npm run build",
    "publish": "npx aws s3 sync dist s3://bitelio.com --profile sre --size-only --delete"
  },
  "cache": {
    "strategy": "inject",
    "swSrc": "./src/sw.js",
    "swDest": "sw.js",
    "globPatterns": [
      "index.html"
    ]
  },
  "devDependencies": {
    "aws-sdk": "^2.585.0",
    "parcel": "^1.12.4",
    "parcel-plugin-sw-cache": "^0.3.1",
    "pug": "^2.0.4",
    "stylus": "^0.54.7"
  }
}
