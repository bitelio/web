{
  "name": "bitelio",
  "version": "1.0.0",
  "description": "web",
  "author": "Guillermo Guirao Aguilar",
  "license": "ISC",
  "scripts": {
    "serve": "parcel src/{index.pug,404.pug}",
    "prebuild": "rm -rf dist",
    "build": "parcel build src/{index.pug,404.pug} --no-source-maps"
  },
  "cache": {
    "strategy": "inject",
    "swSrc": "./src/sw.js",
    "swDest": "sw.js",
    "globPatterns": [
      "index.html"
    ]
  },
  "devDependencies": {
    "aws-sdk": "^2.726.0",
    "parcel": "^1.12.4",
    "parcel-plugin-sw-cache": "^0.3.1",
    "pug": "^2.0.4",
    "stylus": "^0.54.8"
  }
}
